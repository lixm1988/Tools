{
    "name": "AgoraDynamicKey-nodejs",
    "image": "node:22.4",
    "customizations": {
        "vscode": {
            "extensions": [
                "swellaby.node-pack",
                "esbenp.prettier-vscode"
            ],
            "settings": {
                "[javascript]": {
                    "editor.defaultFormatter": "esbenp.prettier-vscode"
                },
                "[typescript]": {
                    "editor.defaultFormatter": "esbenp.prettier-vscode"
                }
            }
        }
    },
    "workspaceMount": "source=${localWorkspaceFolder},target=/app,type=bind",
    "workspaceFolder": "/app"
}